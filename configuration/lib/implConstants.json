{
  "EZCONFIGURATION_ENV_VAR": "EZCONFIGURATION_DIR",
  "EZCONFIGURATION_DEFAULT_DIR": "/etc/sysconfig/ezbake",
  "EZCONFIGURATION_PROPERTIES_FILE_EXTENSION": ".properties",
  "SPLIT_ON_FIRST_EQUALS_REGEXP": "^([^\\=]+)\\=(.*)$",
  "PROPERTY_FILE_REGEX": "\\.properties$",
  "EZBAKE_SHARED_SECRET_ENVIRONMENT_VARIABLE": "ezbake.shared.secret.environment.variable",
  "ENCRYPTED_VALUE_PREFIX": "ENC~(",
  "ENCRYPTED_VALUE_PREFIX_REGEX": "^ENC~\\(",
  "ENCRYPTED_VALUE_POSTFIX": ")~",
  "ENCRYPTED_VALUE_POSTFIX_REGEX": "\\)~$",
  "OPENSHIFT_REPO_DIR_ENV_VAR": "OPENSHIFT_REPO_DIR",
  "DEFAULT_ENCRYPTION_SALT": "bouncycastle",
  "DEFAULT_ENCRYPTION_ITERATIONS": 1000,
  "DEFAULT_BYTES_FOR_SALT": 8
}
